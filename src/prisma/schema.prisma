datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id        Int      @id @default(autoincrement())
    email     String   @unique
    name      String
    username  String   @unique
    password  String
    reg_num   String   @unique
    createdAt DateTime @default(now())
}
